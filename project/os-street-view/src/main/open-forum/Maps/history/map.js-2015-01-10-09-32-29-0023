var canvas;
function initMap() {
  canvas =new Canvas("mapCanvas");
  Giraffe.Interactive.setInteractive(canvas);
  Giraffe.setAnimated(canvas);
  canvas.startAnimation(20,100,true);
  
  tile1 = new Picture(0,0,"/Maps/OSStreetView/Norfolk-Street-View/tf03nw.png");
  tile2 = new Picture(5000,0,"/Maps/OSStreetView/Norfolk-Street-View/tf03ne.png");
  tile3 = new Picture(0,5000,"/Maps/OSStreetView/Norfolk-Street-View/tf03sw.png");
  tile4 = new Picture(5000,5000,"/Maps/OSStreetView/Norfolk-Street-View/tf03se.png");
  map = new Composite(0,0);
  map.add(tile1);
  map.add(tile2);
  canvas.add(map);
  canvas.makeDraggable(map);
}